meta {
  name: 02 - create reserved slot (standalone)
  type: http
  seq: 2
}

post {
  url: {{base_url}}/slots
  body: json
  auth: inherit
}

headers {
  authorization: Bearer {{token}}
}

body:json {
  {
    "eventDate": ":event_date",
    "period": ":slot_period",
    "status": "reserved"
  }
}

settings {
  encodeUrl: true
  timeout: 0
}

