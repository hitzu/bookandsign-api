meta {
  name: create contract from slot (401 - missing token)
  type: http
  seq: 9
}

post {
  url: {{base_url}}/contracts
  body: json
  auth: inherit
}

body:json {
  {
    "slotId": {{slotId}},
    "packages": [
      {
        "packageId": {{packageId}},
        "quantity": 1,
        "source": "package"
      }
    ]
  }
}

settings {
  encodeUrl: true
  timeout: 0
}


