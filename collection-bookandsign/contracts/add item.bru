meta {
  name: add item
  type: http
  seq: 3
}

post {
  url: {{base_url}}/contracts/{{contractId}}/items
  body: json
  auth: inherit
}

headers {
  Authorization: Bearer {{token}}
}

body:json {
  {
    "packageId": {{packageId}},
    "quantity": 1,
    "source": "package",
    "displayAsSaving": false
  }
}

settings {
  encodeUrl: true
  timeout: 0
}


