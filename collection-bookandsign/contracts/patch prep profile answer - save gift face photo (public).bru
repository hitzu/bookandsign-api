meta {
  name: patch prep profile answer - save gift face photo (public)
  type: http
  seq: 26
}

patch {
  url: {{base_url}}/contracts/public/{{contract_token}}/prep-profile/answer?phone={{contract_phone}}
  body: json
  auth: none
}

body:json {
  {
    "questionId": "social-n-1-gift_face_photo",
    "value": {
      "path": "{{prep_asset_path}}",
      "mime": "{{prep_mime}}"
    }
  }
}

settings {
  encodeUrl: true
  timeout: 0
}

