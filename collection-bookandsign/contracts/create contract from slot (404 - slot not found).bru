meta {
  name: create contract from slot (404 - slot not found)
  type: http
  seq: 10
}

post {
  url: {{base_url}}/contracts
  body: json
  auth: inherit
}

headers {
  Authorization: Bearer {{token}}
}

body:json {
  {
    "slotId": 999999,
    "packages": [
      {
        "packageId": {{packageId}},
        "quantity": 1,
        "source": "package"
      }
    ]
  }
}

settings {
  encodeUrl: true
  timeout: 0
}


